{
  "name": "core-ui",
  "version": "1.0.17",
  "packageManager": "pnpm@10.13.1",
  "description": "Core UI components library",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "type": "module",
  "private": true,
  "files": [
    "lib/"
  ],
  "scripts": {
    "dev": "NODE_ENV=development vite --config vite.dev.config.ts",
    "preview": "vite preview --config vite.dev.config.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "pnpm vite:build-production",
    "webpack:build-dev": "NODE_ENV=development webpack --config webpack.config.js --mode=development",
    "webpack:build-production": "rm -rf lib && NODE_ENV=production WEBPACK_OPTIMIZE=true webpack --config webpack.config.js --mode=production",
    "vite:build-dev": " NODE_ENV=development BUILD_TYPE=main vite build --config vite.main.config.ts",
    "vite:build-production": "pnpm vite:build-main && pnpm vite:build-components",
    "vite:build-main": " NODE_ENV=production BUILD_TYPE=main vite build --config vite.main.config.ts",
    "vite:build-components": " NODE_ENV=production BUILD_TYPE=components vite build --config vite.components.config.ts",
    "vite:build-all": " rm -rf lib && NODE_ENV=production BUILD_TYPE=main vite build --config vite.main.config.ts && NODE_ENV=production BUILD_TYPE=components vite build --config vite.components.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "prettier src/ --write",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "prepare": "husky",
    "quality": "pnpm format && pnpm lint && pnpm typecheck"
  },
  "peerDependencies": {
    "eslint": "*",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19",
    "typescript": "*"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "react": "^19",
    "react-dom": "^19",
    "react-refresh": "^0.11.0",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@eslint/js": "^9.34.0",
    "@hookform/resolvers": "^5.0.1",
    "@ngneat/falso": "^7.3.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@redux-devtools/extension": "^3.3.0",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/react-vite": "^9.1.5",
    "@storybook/test": "^8.6.14",
    "@svgr/webpack": "^5.5.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/vite": "^4.1.12",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.19.11",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^27.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^7.0.2",
    "browserslist": "^4.18.1",
    "camelcase": "^6.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "compression-webpack-plugin": "^11.1.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "dotenv": "^16.5.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.5",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "globals": "^15.15.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.3",
    "jest-resolve": "^27.4.2",
    "jest-watch-typeahead": "^1.0.0",
    "lightningcss": "^1.29.3",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.0.1",
    "prettier": "3.3.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prompts": "^2.4.2",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "react-hook-form": "^7.56.1",
    "resolve": "^1.20.0",
    "resolve-url-loader": "^4.0.0",
    "rimraf": "^6.0.1",
    "sass-loader": "^12.3.0",
    "semver": "^7.3.5",
    "source-map-loader": "^3.0.0",
    "storybook": "^9.1.5",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.12",
    "terser-webpack-plugin": "^5.2.5",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.3",
    "vite-bundle-analyzer": "^1.2.3",
    "vite-plugin-checker": "^0.10.3",
    "vite-plugin-dts": "^4.5.4",
    "web-vitals": "^2.1.4",
    "webpack": "^5.99.7",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-manifest-plugin": "^4.0.2",
    "workbox-webpack-plugin": "^6.4.1",
    "zod": "^3.24.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "jsdom",
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|sass|scss)$": "identity-obj-proxy",
      "^@combobox/(.*)$": "<rootDir>/src/@combobox/$1",
      "^@input/(.*)$": "<rootDir>/src/@input/$1",
      "^@label/(.*)$": "<rootDir>/src/@label/$1",
      "^@icons/(.*)$": "<rootDir>/src/@icons/$1",
      "^@hook/(.*)$": "<rootDir>/src/@hook/$1",
      "^@button/(.*)$": "<rootDir>/src/@button/$1",
      "^@zustand/(.*)$": "<rootDir>/src/@zustand/$1",
      "^@utils/(.*)$": "<rootDir>/src/@utils/$1"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.8.2",
    "pnpm": ">=10.12.1"
  },
  "overrides": {
    "storybook": "^9.1.5"
  }
}
